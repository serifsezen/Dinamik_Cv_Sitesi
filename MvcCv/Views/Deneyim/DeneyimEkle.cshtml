
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />
<form method="post">
    <h5>Başlık</h5>
    <input type="text" name="Baslik" class="form-control" placeholder="Başlık" />
    <br />
    <h5>Alt Başlık</h5>
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt Başlık" />
    <br />
    <h5>Tarih</h5>
    <input type="text" name="Tarih" class="form-control" placeholder="Çalışma Tarihi" />
    <br />
    <h5>Açıklama</h5>
    <textarea placeholder="Açıklama" name="Aciklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>
